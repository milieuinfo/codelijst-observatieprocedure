        skos:inScheme    vlcs:observatieprocedure ;
        skos:notation    "WAC/III/C/040" ;
        skos:prefLabel   "Potentiometrische bepaling van opgelost en in zuur milieu oplosbare sulfiden"@nl ;
        dcat:theme       gemet:15179 .

