        skos:inScheme      vlcs:observatieprocedure ;
        skos:notation      "CMA/1/A.8" ;
        skos:prefLabel     "Staalname van hopen/partijen bodemmaterialen"@nl ;
        skos:topConceptOf  vlcs:observatieprocedure ;
        dcat:theme         gemet:7884 .

